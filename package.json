    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "node-mocks-http": "^1.8.1",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  }
